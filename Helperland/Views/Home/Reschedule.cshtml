@model Helperland_projectContext.Models.ServiceRequest
             <input type="hidden" id="id" asp-for="ServiceRequestId"/>
                <div>
                    <span class="modal-title pe-3" id="exampleModalLabel">Reschedule Service Request</span>
                    <button type="button" class="btn-close x" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
            <span class="mt-2 model-head">Select New Date & Time</span><br>
            <label><input class="datein" type="date" id="resdate" asp-for="ServiceStartDate"/></label>
                <span>
                <select class="reschedule-time ms-2 me-2 mb-3" id="time">
                    <option selected>8:00 AM</option>
                    <option value="1">9:00 AM</option>
                    <option value="1">10:00 AM</option>
                    <option value="1">11:00 AM</option>
                    <option value="1">12:00</option>
                    <option value="1">1:00 PM</option>
                    <option value="1">2:00 PM</option>
                    <option value="1">3:00 PM</option>
                    <option value="1">4:00 PM</option>
                    <option value="1">5:00 PM</option>
                    <option value="1">6:00 PM</option>
                    <option value="1">7:00 PM</option>
                    <option value="1">8:00 PM</option>
                </select>
                </span>
                <button class="update" id="updatetime" @*onclick="update()"*@ type="button">Update</button>
